@model IEnumerable<MvcCecep.ccempresa>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th style="text-align:center">
            Identificación
        </th>
        <th style="text-align:center">
            Descripción
        </th>
        <th style="text-align:center">
            Direccion
        </th>
        <th style="text-align:center">
            Telefono
        </th>
        <th style="text-align:center">
            Celular
        </th>
        <th style="text-align:center">
            Fecha Creación
        </th>

        <th style="text-align:center; width:100px;">
            Opciones
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td style="text-align:center;">
                @item.nit - @item.dv
            </td>
            <td>
                @item.descripcion
            </td>
            <td>
                @item.direccion
            </td>
            <td>
                @item.telefono
            </td>
            <td>
                @item.celular
            </td>
            <td style="text-align:center;">
                @Html.DisplayFor(modelItem => item.fechacreacion)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ccempresaid }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ccempresaid })
            </td>
        </tr>
    }

</table>
