@model MvcCecep.ccnoticia

@{
    ViewBag.Title = "Eliminar Noticia";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="row">
        <div class="col-md-5">
            @Html.LabelFor(model => model.fechapublic, "Fecha Publicación")
        </div>
        <div class="col-md-5">
            @Html.DisplayFor(model => model.fechapublic)
        </div>
    </div>



    <div class="row">
        <div class="col-md-5">
            @Html.LabelFor(model => model.titulo, "Titulo")
        </div>
        <div class="col-md-5">
            @Html.DisplayFor(model => model.titulo)
        </div>
    </div>

    <div class="row">
        <div class="col-md-5">
            @Html.LabelFor(model => model.descripcion)
        </div>
        <div class="col-md-5">
            @Html.DisplayFor(model => model.descripcion)
        </div>
    </div>


    <div class="row" style="margin-top:20px;">
        <div class="col-md-2">
            <button type="submit" class="btn btn-danger">Eliminar</button>
        </div>
        <div class="col-md-2">
            @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-default" })
        </div>
    </div>

}